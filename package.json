{
  "name": "lawd",
  "version": "0.4.0",
  "description": "Video streaming desktop application",
  "main": "main.js",
  "scripts": {
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "package": "cross-env NODE_ENV=production node package.js --asar",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "bootstrap": "^4.0.0-alpha.2",
    "filesize": "^3.2.1",
    "font-awesome": "^4.6.3",
    "googleapis": "^7.1.0",
    "iconv-lite": "^0.4.13",
    "jquery": "^2.2.2",
    "mime": "^1.3.4",
    "moment": "^2.13.0",
    "opensubtitles-api": "^3.1.1",
    "parse": "^1.8.5",
    "srt2vtt": "^1.3.1",
    "tether": "^1.2.0",
    "underscore": "^1.8.3",
    "video.js": "^5.10.4",
    "vtt.js": "^0.12.1",
    "vue": "^1.0.24",
    "vue-i18n": "^4.0.0",
    "vue-infinite-scroll": "^0.2.3",
    "vue-resource": "^0.7.2",
    "vue-router": "^0.7.13",
    "webtorrent": "^0.94.3",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.1.5",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.8.5",
    "fs-jetpack": "^0.9.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "1.2.2",
    "vue-loader": "^8.2.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "git+https://github.com/glenjamin/webpack-hot-middleware",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
